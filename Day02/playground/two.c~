void ft_print_alphabet(void);

void ft_print_alphabet(void) {
	char first_letter = 'a';
	char last_letter = 'z';
	for (char i = first_letter; i <= last_letter; i++){	
		ft_putchar(i);
	}
	ft_putchar('\n');
}
